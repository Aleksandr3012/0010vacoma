extends layout/main.pug


include ../blocks/sContent/_sContent.pug
include ../blocks/sContact/_sContact.pug 
include ../blocks/s-img/_s-img.pug 
include ../blocks/s-news/_s-news.pug 

block variables
	- let  mainPage= null
block title
	- let  title= 'Зубчатая передача Турборедуктора М-430 в составе газоперекачивающего агрегата ТКА-Ц-16-76для ПАО «СИБУР Холдинг»'
	title= title
block description 
	meta(name="description", content= "Зубчатая передача Турборедуктора М-430 в составе газоперекачивающего агрегата ТКА-Ц-16-76для ПАО «СИБУР Холдинг»" )

block keywords
	meta(name="Keywords", content= "вакома, вакома магитогорск, вакома рус, vakoma, проектирование редукторной техники, изготовление редуктора, проектирование редуктора, проектирование валов редуктора, проектирование одноступенчатого редуктора, проектирование планетарного редуктора, проектирование редуктора, проектирование цилиндрического редуктора, проектирование червячного редуктора" )
block pagecontent 
	mixin menu()
		+e.UL.nav(itemscope itemtype="http://schema.org/SiteNavigationElement")
			+e.LI.item( itemprop="item" ): +e.A.link(href="index.html"  itemprop="url") 
				span Главная
			+e.LI.item( itemprop="item" ): +e.A.link(href="our-company.html"  itemprop="url") 
				span О компании
			+e.LI.item( itemprop="item" ): +e.A.link(href="tehnicheskie-vozmojnosti.html"  itemprop="url") 
				span Технические возможности
			+e.LI.item.dropdown( itemprop="item" ): +e.link 
				span Редукторы и&nbsp;мультипликаторы
				+b.UL.drop
					+e.LI.item
						+e.A.link(href="tyajelonagrujennie-reduktori.html" itemprop="url") Тяжелонагруженные редукторы
					+e.LI.item
						+e.A.link(href="turboreduktori.html" itemprop="url") Мультипликаторы (турборедукторы)
			+e.LI.item( itemprop="item" ): +e.A.link(href="zubchatie-peredachi.html"  itemprop="url") 
				span Зубчатые передачи и&nbsp;комплектующие
			+e.LI.item( itemprop="item" ): +e.A.link.text-warning(href="importozameshenie.html"  itemprop="url")
				span Импортозамещение
			+e.LI.item( itemprop="item" ): +e.A.link(href="kontakti.html"  itemprop="url") 
				span Контакты
	//- +header()
	+mob-mnu()
		+menu()
	+topLine()
	+top-nav
		+menu()

	+bread()
	
	+text()(class="text-block--inner")
		.container
			p.strong Зубчатая передача Турборедуктора М-430 в составе газоперекачивающего агрегата ТКА-Ц-16-76для ПАО «СИБУР Холдинг»
			p В рамках реализации программы ремонтов основного технологического оборудования ПАО «СИБУР Холдинг» наша компания выполнила поставку зубчатой передачи турборедуктора М-430 в составе газоперекачивающего агрегата ТКА-Ц-16/76
			p Разработка конструкторской документации была выполнена на основании проведённых замеров специалистами ООО «ВАКОМА РУС» на производственной площадке Заказчика. Зубчатая передача была изготовлена с учетом посадочных мест корпуса редуктора, находящегося в эксплуатации.
			p При изготовлении были использованы усовершенствованные NiCrMo марки сплавов, имеющие повышенные характеристики, с проведением термообработки поверхности зубьев – цементация (твердость 57 HRC), зубошлифовке и динамической балансировке с установленными на валы зубчатыми втулками.
			p Техническими специалистами ООО «ВАКОМА РУС» были проведены работы по шеф-монтажу и пуско-наладке турборедуктора после проведённых ремонтных работ на площадке Заказчика.



	//- +s-img-news-2016-08-11
	+s-img-news-body()
		+slidImg('img/@2x/2019-09-16-news-01.jpg', 'img/@2x/2019-09-16-news-xl-01.jpg', 'вакома рус')
		+slidImg('img/@2x/2019-09-16-news-02.jpg', 'img/@2x/2019-09-16-news-xl-02.jpg', 'вакома рус')
		+slidImg('img/@2x/2019-09-16-news-03.jpg', 'img/@2x/2019-09-16-news-xl-03.jpg', 'вакома рус')
		+slidImg('img/@2x/2019-09-16-news-04.jpg', 'img/@2x/2019-09-16-news-xl-04.jpg', 'вакома рус')
		+slidImg('img/@2x/2019-09-16-news-05.jpg', 'img/@2x/2019-09-16-news-xl-05.jpg', 'вакома рус')
		+slidImg('img/@2x/2019-09-16-news-06.jpg', 'img/@2x/2019-09-16-news-xl-06.jpg', 'вакома рус')
	.container
		.strong Технические характеристики:
		.table-wrap
			table
				tr( style="background-color:#16437c; color: #fff").text-center
