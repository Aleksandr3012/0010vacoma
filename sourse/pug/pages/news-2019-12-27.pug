extends layout/main.pug


include ../blocks/sContent/_sContent.pug
include ../blocks/sContact/_sContact.pug 
include ../blocks/s-img/_s-img.pug 
include ../blocks/s-news/_s-news.pug 

block variables
	- let  mainPage= null
block title
	- let  title= 'Ремонтный комплект зубчатой пары мультипликатора Р-5000 компрессора К-890-121-1 для ПАО «СИБУР Холдинг»'
	title= title
block description 
	meta(name="description", content= "Ремонтный комплект зубчатой пары мультипликатора Р-5000 компрессора К-890-121-1 для ПАО «СИБУР Холдинг»" )

block keywords
	meta(name="Keywords", content= "вакома, вакома магитогорск, вакома рус, vakoma, проектирование редукторной техники, изготовление редуктора, проектирование редуктора, проектирование валов редуктора, проектирование одноступенчатого редуктора, проектирование планетарного редуктора, проектирование редуктора, проектирование цилиндрического редуктора, проектирование червячного редуктора" )
block pagecontent 
	mixin menu()
		+e.UL.nav(itemscope itemtype="http://schema.org/SiteNavigationElement")
			+e.LI.item( itemprop="item" ): +e.A.link(href="index.html"  itemprop="url") 
				span Главная
			+e.LI.item( itemprop="item" ): +e.A.link(href="our-company.html"  itemprop="url") 
				span О компании
			+e.LI.item( itemprop="item" ): +e.A.link(href="tehnicheskie-vozmojnosti.html"  itemprop="url") 
				span Технические возможности
			+e.LI.item.dropdown( itemprop="item" ): +e.link 
				span Редукторы и&nbsp;мультипликаторы
				+b.UL.drop
					+e.LI.item
						+e.A.link(href="tyajelonagrujennie-reduktori.html" itemprop="url") Тяжелонагруженные редукторы
					+e.LI.item
						+e.A.link(href="turboreduktori.html" itemprop="url") Мультипликаторы (турборедукторы)
			+e.LI.item( itemprop="item" ): +e.A.link(href="zubchatie-peredachi.html"  itemprop="url") 
				span Зубчатые передачи и&nbsp;комплектующие
			+e.LI.item( itemprop="item" ): +e.A.link.text-warning(href="importozameshenie.html"  itemprop="url")
				span Импортозамещение
			+e.LI.item( itemprop="item" ): +e.A.link(href="kontakti.html"  itemprop="url") 
				span Контакты
	//- +header()
	+mob-mnu()
		+menu()
	+topLine()
	+top-nav
		+menu()

	+bread()
	
	+text()(class="text-block--inner")
		.container
			p.strong Ремонтный комплект зубчатой пары мультипликатора Р-5000 компрессора К-890-121-1 для ПАО «СИБУР Холдинг»
			p ООО «ВАОКМА РУС» выполнило поставку ремонтного комплекта зубчатой пары мультипликатора Р-5000 компрессора К-890-121-1. Изготовление осуществлялось на основании разработанной конструкторской документации специалистами нашей компании.
			p При изготовлении были использованы усовершенствованные NiCrMo марки сплавов, имеющие повышенные характеристики, с проведением термообработки поверхности зубьев – цементация (твердость 57 HRC), зубошлифовке и динамической балансировке. Зубчатая передача была изготовлена с учетом посадочных мест корпуса редуктора, находящегося в эксплуатации.



	+s-img-news-27-12-2019()
	//-+s-img-news-body()
		+slidImg('img/@2x/2019-01-25-news-01.jpg', 'img/@2x/2019-01-25-news-xl-01.jpg', 'вакома рус')
	.container
		.strong Технические характеристики:
		.table-wrap
			table
				tr( style="background-color:#16437c; color: #fff").text-center
					th( ) Параметр
					th Быстроходный вал
					th  Тихоходный вал
	
				tr.bg-light
					td  Число зубьев
					td.text-center 35
					td.text-center 62
	
				tr.bg-light
					td  Частота вращения ротора (ЦНД/ЦВД), об/мин
					td(colspan="2").text-center 5 240 / 9 270
	
				tr.bg-light
					td  Угол наклона зубьев
					td(colspan="2").text-center 29°
	
				tr.bg-light
					td  Модуль, мм
					td(colspan="2").text-center 5
	
				tr.bg-light
					td  Масса, кг
					td(colspan="2").text-center 502,98
	
	
	

 
	
	.pb-5
		+bread()
	+footer()