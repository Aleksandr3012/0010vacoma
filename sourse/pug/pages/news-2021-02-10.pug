extends layout/main.pug


include ../blocks/sContent/_sContent.pug
include ../blocks/sContact/_sContact.pug 
include ../blocks/s-img/_s-img.pug 
include ../blocks/s-news/_s-news.pug 

block variables
	- let  mainPage= null
block title
	- let  title= 'Экспериментальная поставка тележек расплава (аналог TROLLEY&BUCKET, импортозамещение США) для ПАО «СИБУР Холдинг»'
	title= title
block description 
	meta(name="description", content= "В рамках реализации программы по импортозамещению основного технологического оборудования ПАО «СИБУР Холдинг» специалистами нашей компании был выполнен экспериментальный проект по разработке и изготовлению партии тележек (аналог TROLLEY&BUCKET , США), использующихся в технологическом процессе для сбора расплава полимера при пуске экструдера." )

block keywords
	meta(name="Keywords", content= "вакома, вакома магитогорск, вакома рус, vakoma, проектирование редукторной техники, изготовление редуктора, проектирование редуктора, проектирование валов редуктора, проектирование одноступенчатого редуктора, проектирование планетарного редуктора, проектирование редуктора, проектирование цилиндрического редуктора, проектирование червячного редуктора" )
block pagecontent 
	mixin menu()
		+e.UL.nav(itemscope itemtype="http://schema.org/SiteNavigationElement")
			+e.LI.item.active( itemprop="item" ): +e.A.link(href="index.html"  itemprop="url") 
				span Главная
			+e.LI.item( itemprop="item" ): +e.A.link(href="our-company.html"  itemprop="url") 
				span О компании
			+e.LI.item( itemprop="item" ): +e.A.link(href="tehnicheskie-vozmojnosti.html"  itemprop="url") 
				span Технические возможности
			+e.LI.item.dropdown( itemprop="item" ): +e.link 
				span Редукторы и&nbsp;мультипликаторы
				+b.UL.drop
					+e.LI.item
						+e.A.link(href="tyajelonagrujennie-reduktori.html" itemprop="url") Тяжелонагруженные редукторы
					+e.LI.item
						+e.A.link(href="turboreduktori.html" itemprop="url") Мультипликаторы (турборедукторы)
			+e.LI.item( itemprop="item" ): +e.A.link(href="zubchatie-peredachi.html"  itemprop="url") 
				span Зубчатые передачи и&nbsp;комплектующие
			+e.LI.item( itemprop="item" ): +e.A.link.text-warning(href="importozameshenie.html"  itemprop="url")
				span Импортозамещение
			+e.LI.item( itemprop="item" ): +e.A.link(href="kontakti.html"  itemprop="url") 
				span Контакты
	//- +header()
	+mob-mnu()
		+menu()
	+topLine()
	+top-nav
		+menu()

	+bread()
	
	+text()(class="text-block--inner")
		.container
			p.strong Экспериментальная поставка тележек расплава (аналог TROLLEY&BUCKET, импортозамещение США) для ПАО «СИБУР Холдинг».
			p В рамках реализации программы по импортозамещению основного технологического оборудования ПАО «СИБУР Холдинг» специалистами нашей компании был выполнен экспериментальный проект по разработке и изготовлению партии тележек (аналог TROLLEY&BUCKET , США), использующихся в технологическом процессе для сбора расплава полимера при пуске экструдера.
			p Конструкторским бюро ООО «ВАКОМА РУС» были выполнены все необходимые расчеты и разработана конструкторская документация с учетом всех требований Заказчика.



	//-+s-img-news-2020-02-18
	+s-img-news-body()
		+slidImg('img/@2x/2021-02-10-news-01.jpg', 'img/@2x/2021-02-10-news-xl-01.jpg', 'вакома рус')
		+slidImg('img/@2x/2021-02-10-news-02.jpg', 'img/@2x/2021-02-10-news-xl-02.jpg', 'вакома рус')
		+slidImg('img/@2x/2021-02-10-news-03.jpg', 'img/@2x/2021-02-10-news-xl-03.jpg', 'вакома рус')
		+slidImg('img/@2x/2021-02-10-news-04.jpg', 'img/@2x/2021-02-10-news-xl-04.jpg', 'вакома рус')
		+slidImg('img/@2x/2021-02-10-news-05.jpg', 'img/@2x/2021-02-10-news-xl-05.jpg', 'вакома рус')
		+slidImg('img/@2x/2021-02-10-news-06.jpg', 'img/@2x/2021-02-10-news-xl-06.jpg', 'вакома рус')
		+slidImg('img/@2x/2021-02-10-news-07.jpg', 'img/@2x/2021-02-10-news-xl-07.jpg', 'вакома рус')

	.container
		.strong Технические характеристики:
		.table-wrap
			table
				tr( style="background-color:#16437c; color: #fff").text-center
					th( ) Параметр
					th Значение
	
				tr.bg-light
					td  Грузоподъемность, кг
					td.text-center 350
	
				tr.bg-light
					td  Объем контейнера, л
					td.text-center 250
	
				tr.bg-light
					td  Материал контейнера
					td.text-center 12Х18Н10Т 
	
				tr.bg-light
					td  Температура расплава, выгружаемого в контейнер, °С 
					td.text-center 450
	
				tr.bg-light
					td  Угол наклона контейнера при выгрузке, градус
					td.text-center 40

 
	
	.pb-5
		+bread()
	+footer()