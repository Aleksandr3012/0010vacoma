extends layout/main.pug


include ../blocks/sContent/_sContent.pug
include ../blocks/sContact/_sContact.pug 
include ../blocks/s-img/_s-img.pug 
include ../blocks/s-news/_s-news.pug 

block variables
	- let  mainPage= null
block title
	- let  title= 'Вторая партия промежуточных валов специального исполнения - аналог промвала Rexnord вентиляторной установки Linde (импортозамещение США, Германия) для ПАО «СИБУР Холдинг»'
	title= title
block description 
	meta(name="description", content= "В рамках реализации программы по импортозамещению комплектующих основного технологического оборудования ПАО «СИБУР Холдинг» нашей компанией была изготовлена и поставлена вторая партия промежуточных валов специального исполнения (аналог Rexnord, Германия) для вентиляторной установки Linde (США)" )

block keywords
	meta(name="Keywords", content= "вакома, вакома магитогорск, вакома рус, vakoma, проектирование редукторной техники, изготовление редуктора, проектирование редуктора, проектирование валов редуктора, проектирование одноступенчатого редуктора, проектирование планетарного редуктора, проектирование редуктора, проектирование цилиндрического редуктора, проектирование червячного редуктора" )
block pagecontent 
	mixin menu()
		+e.UL.nav(itemscope itemtype="http://schema.org/SiteNavigationElement")
			+e.LI.item.active( itemprop="item" ): +e.A.link(href="index.html"  itemprop="url") 
				span Главная
			+e.LI.item( itemprop="item" ): +e.A.link(href="our-company.html"  itemprop="url") 
				span О компании
			+e.LI.item( itemprop="item" ): +e.A.link(href="tehnicheskie-vozmojnosti.html"  itemprop="url") 
				span Технические возможности
			+e.LI.item.dropdown( itemprop="item" ): +e.link 
				span Редукторы и&nbsp;мультипликаторы
				+b.UL.drop
					+e.LI.item
						+e.A.link(href="tyajelonagrujennie-reduktori.html" itemprop="url") Тяжелонагруженные редукторы
					+e.LI.item
						+e.A.link(href="turboreduktori.html" itemprop="url") Мультипликаторы (турборедукторы)
			+e.LI.item( itemprop="item" ): +e.A.link(href="zubchatie-peredachi.html"  itemprop="url") 
				span Зубчатые передачи и&nbsp;комплектующие
			+e.LI.item( itemprop="item" ): +e.A.link.text-warning(href="importozameshenie.html"  itemprop="url")
				span Импортозамещение
			+e.LI.item( itemprop="item" ): +e.A.link(href="kontakti.html"  itemprop="url") 
				span Контакты
	//- +header()
	+mob-mnu()
		+menu()
	+topLine()
	+top-nav
		+menu()

	+bread()
	
	+text()(class="text-block--inner")
		.container
			p.strong Вторая партия промежуточных валов специального исполнения - аналог промвала Rexnord вентиляторной установки Linde (импортозамещение США, Германия) для ПАО «СИБУР Холдинг»
			p В рамках реализации программы по импортозамещению комплектующих основного технологического оборудования ПАО «СИБУР Холдинг» нашей компанией была изготовлена и поставлена вторая партия промежуточных валов специального исполнения (аналог Rexnord, Германия) для вентиляторной установки Linde (США).
			p Изготовление было выполнено на основании расчётов конструкторского бюро ООО «ВАКОМА РУС» и разработанной конструкторской документации с учетом применения легированных нержавеющих сталей взамен оригинальных композиционных с проведением динамической балансировки по классу G2,5 ГОСТ ИСО 1940-1-2007.
			p Промвал был изготовлен в контрактные сроки и принят в эксплуатацию.
			

	//-+s-img-news-2020-02-18
	+s-img-news-body()
		+slidImg('img/@2x/2021-02-01-news-01.jpg', 'img/@2x/2021-02-01-news-xl-01.jpg', 'вакома рус')
		+slidImg('img/@2x/2021-02-01-news-02.jpg', 'img/@2x/2021-02-01-news-xl-02.jpg', 'вакома рус')
		+slidImg('img/@2x/2021-02-01-news-03.jpg', 'img/@2x/2021-02-01-news-xl-03.jpg', 'вакома рус')
		+slidImg('img/@2x/2021-02-01-news-04.jpg', 'img/@2x/2021-02-01-news-xl-04.jpg', 'вакома рус')

	.container
		.strong Технические характеристики:
		.table-wrap
			table
				tr( style="background-color:#16437c; color: #fff").text-center
					th( ) Параметр
					th Значение
	
				tr.bg-light
					td  Мощность двигателя, кВт
					td.text-center 200
	
				tr.bg-light
					td  Номинальный крутящий момент двигателя, Нм
					td.text-center 1 282
	
				tr.bg-light
					td  Скорость вращения двигателя, об/мин
					td.text-center 1 492
	
				tr.bg-light
					td  Длинна, мм 
					td.text-center 3 960

 
	
	.pb-5
		+bread()
	+footer()