					td(colspan="2").text-center 1 475
	
	
	
				tr.bg-light
					td  Число оборотов ведомого вала, об/мин
					td(colspan="2").text-center 9 130
	
	
	
				tr.bg-light
					td  Число зубьев
					td.text-center 33
					td.text-center 209

	
				tr.bg-light
					td  Межосевое расстояние, мм
					td(colspan="2").text-center 405 ± 0,03
	
		
				tr.bg-light
					td  Угол наклона зубьев
					td(colspan="2").text-center 25°48’
	
		
				tr.bg-light
					td  Модуль, мм
					td(colspan="2").text-center 3
	
		
				tr.bg-light
					td  Общая масса, кг
					td(colspan="2").text-center 207,72
	
						
	
	


 
	
	.pb-5
		+bread()
	+footer()