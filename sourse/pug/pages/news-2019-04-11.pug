extends layout/main.pug


include ../blocks/sContent/_sContent.pug
include ../blocks/sContact/_sContact.pug 
include ../blocks/s-img/_s-img.pug 
include ../blocks/s-news/_s-news.pug 

block variables
	- let  mainPage= null
block title
	- let  title= 'Реверс-инжиниринг и поставка комплекта ускоряющей передачи для газодувки газа регенерации В-102 (импортозамещение MITSUBISHI, Япония) для ПАО «СИБУР Холдинг»'
	title= title
block description 
	meta(name="description", content= "Реверс-инжиниринг и поставка комплекта ускоряющей передачи для газодувки газа регенерации В-102 (импортозамещение MITSUBISHI, Япония) для ПАО «СИБУР Холдинг»" )

block keywords
	meta(name="Keywords", content= "вакома, вакома магитогорск, вакома рус, vakoma, проектирование редукторной техники, изготовление редуктора, проектирование редуктора, проектирование валов редуктора, проектирование одноступенчатого редуктора, проектирование планетарного редуктора, проектирование редуктора, проектирование цилиндрического редуктора, проектирование червячного редуктора" )
block pagecontent 
	mixin menu()
		+e.UL.nav(itemscope itemtype="http://schema.org/SiteNavigationElement")
			+e.LI.item( itemprop="item" ): +e.A.link(href="index.html"  itemprop="url") 
				span Главная
			+e.LI.item( itemprop="item" ): +e.A.link(href="our-company.html"  itemprop="url") 
				span О компании
			+e.LI.item( itemprop="item" ): +e.A.link(href="tehnicheskie-vozmojnosti.html"  itemprop="url") 
				span Технические возможности
			+e.LI.item.dropdown( itemprop="item" ): +e.link 
				span Редукторы и&nbsp;мультипликаторы
				+b.UL.drop
					+e.LI.item
						+e.A.link(href="tyajelonagrujennie-reduktori.html" itemprop="url") Тяжелонагруженные редукторы
					+e.LI.item
						+e.A.link(href="turboreduktori.html" itemprop="url") Мультипликаторы (турборедукторы)
			+e.LI.item( itemprop="item" ): +e.A.link(href="zubchatie-peredachi.html"  itemprop="url") 
				span Зубчатые передачи и&nbsp;комплектующие
			+e.LI.item( itemprop="item" ): +e.A.link.text-warning(href="importozameshenie.html"  itemprop="url")
				span Импортозамещение
			+e.LI.item( itemprop="item" ): +e.A.link(href="kontakti.html"  itemprop="url") 
				span Контакты
	//- +header()
	+mob-mnu()
		+menu()
	+topLine()
	+top-nav
		+menu()

	+bread()
	
	+text()(class="text-block--inner")
		.container
			p.strong Реверс-инжиниринг и поставка комплекта ускоряющей передачи для газодувки газа регенерации В-102 (импортозамещение MITSUBISHI, Япония) для ПАО «СИБУР Холдинг»
			p В рамках программы по импортозамещению основного технологического оборудования ООО «ВАКОМА РУС» выполнило работы по разработке рабочей конструкторской документации, изготовлению и поставке комплект ускоряющей передачи для газодувки газа регенерации В-102 производства MITSUBISHI. 
			p Разработка документации была выполнена по результатам проведенных замеров и изучения исходной документации на площадке Заказчика. При изготовлении были использованы усовершенствованные NiCrMo марки сплавов, имеющие повышенные характеристики, с проведением термообработки поверхности зубьев – цементация (твердость 60HRC, h=1,4 мм), зубошлифовки и динамической балансировки. 



	//- +s-img-news-2016-08-11
