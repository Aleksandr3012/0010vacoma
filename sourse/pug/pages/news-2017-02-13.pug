extends layout/main.pug


include ../blocks/sContent/_sContent.pug
include ../blocks/sContact/_sContact.pug 
include ../blocks/s-img/_s-img.pug 
include ../blocks/s-news/_s-news.pug 

block variables
	- let  mainPage= null
block title
	- let  title= 'Новость'
	title= title
block description 
	meta(name="description", content= "Проектирование и изготовление редукторной техники на основании конструкторской документации Заказчиком, а также по собственному инжинирингу" )

block keywords
	meta(name="Keywords", content= "вакома, вакома магитогорск, вакома рус, vakoma, проектирование редукторной техники, изготовление редуктора, проектирование редуктора, проектирование валов редуктора, проектирование одноступенчатого редуктора, проектирование планетарного редуктора, проектирование редуктора, проектирование цилиндрического редуктора, проектирование червячного редуктора" )
block pagecontent 
	mixin menu()
		+e.UL.nav(itemscope itemtype="http://schema.org/SiteNavigationElement")
			+e.LI.item( itemprop="item" ): +e.A.link(href="index.html"  itemprop="url") 
				span Главная
			+e.LI.item( itemprop="item" ): +e.A.link(href="tehnicheskie-vozmojnosti.html"  itemprop="url") 
				span Технические возможности
			+e.LI.item.active( itemprop="item" ): +e.A.link(href="novosti-2016.html"  itemprop="url") 
				span Новости
			+e.LI.item.dropdown( itemprop="item" ): +e.link 
				span Редукторы и&nbsp;мультипликаторы
				+b.UL.drop
					+e.LI.item
						+e.A.link(href="tyajelonagrujennie-reduktori.html" itemprop="url") Тяжелонагруженные редукторы
					+e.LI.item
						+e.A.link(href="turboreduktori.html" itemprop="url") Мультипликаторы (турборедукторы)
			+e.LI.item( itemprop="item" ): +e.A.link(href="zubchatie-peredachi.html"  itemprop="url") 
				span Зубчатые передачи и&nbsp;комплектующие
			+e.LI.item( itemprop="item" ): +e.A.link.text-warning(href="importozameshenie.html"  itemprop="url")
				span Импортозамещение
			+e.LI.item( itemprop="item" ): +e.A.link(href="kontakti.html"  itemprop="url") 
				span Контакты
	//- +header()
	+mob-mnu()
		+menu()
	+topLine()
	+top-nav
		+menu()

	+bread()
	
	+text()(class="text-block--inner")
		.container
			h1.strong  Турборедукторы Р-3000, Р-6000 компрессора К&nbsp;380-103-1 для ООО &laquo;Цемек Минералс&raquo;
			p В&nbsp;рамках реализации ремонтной программы одного из&nbsp;заказчиков ООО &laquo;Цемек Минералс&raquo; нефтегазовой промышленности специалистами нашей компании были разработаны и&nbsp;изготовлены турборедукторы для компрессора К&nbsp;380-103-1.
			p В&nbsp;рамках реализации проекта технические специалисты ООО &laquo;ВАКОМА РУС&raquo; посетили производственную площадку Заказчика для проведения замеров всех основных геометрических и&nbsp;присоединительных размеров и&nbsp;ознакомления с&nbsp;имеющейся технической документацией.
			p По&nbsp;требованию заказчика передаточные числа турборедукторов были изменены. На&nbsp;основании выполненных расчетов была выпущена на&nbsp;согласование рабочая конструкторская документация (все посадочные и&nbsp;присоединительные размеры сохранены).
			ul
				li Корпуса редукторов&nbsp;&mdash; крутильно-жесткие, недеформируемые, сварной конструкции (рассчитан в&nbsp;соответствии с&nbsp;МКЭ).
				li Зубчатые зацепления закаленные цементованно-шлифованные с&nbsp;динамической балансировкой степени точности G1,6 (DIN ISO 2768-m). 
				li Исполнение: цилиндрический.
				li Материал: а) вал-шестерня 16MnCr5;&nbsp;б) зубчатое колесо 16MnCr5; вал C45 +N.
				li Редукторы изготовлены с&nbsp;подшипниками скольжения.
				li Функционально важные места снабжены контрольно-измерительными приборами с&nbsp;цифровыми датчиками сигналов.
			p Турборедукторы изготовлены с&nbsp;учетом монтажа и&nbsp;установки на&nbsp;существующие фундаменты редукторов Р-3000 и&nbsp;Р-6000.
			p Перед отгрузкой была выполнена обкатка редукторов на&nbsp;испытательном стенде со&nbsp;снятием основных технологических параметров
