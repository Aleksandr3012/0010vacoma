		+slidImg('img/@2x/2017-02-13-news-01.jpg', 'img/@2x/2017-02-13-news-xl-01.jpg')
		+slidImg('img/@2x/2017-02-13-news-10.jpg', 'img/@2x/2017-02-13-news-xl-10.jpg')
		+slidImg('img/@2x/2017-02-13-news-02.jpg', 'img/@2x/2017-02-13-news-xl-02.jpg')
		+slidImg('img/@2x/2017-02-13-news-03.jpg', 'img/@2x/2017-02-13-news-xl-03.jpg')
		+slidImg('img/@2x/2017-02-13-news-04.jpg', 'img/@2x/2017-02-13-news-xl-04.jpg')
		+slidImg('img/@2x/2017-02-13-news-05.jpg', 'img/@2x/2017-02-13-news-xl-05.jpg')
		+slidImg('img/@2x/2017-02-13-news-06.jpg', 'img/@2x/2017-02-13-news-xl-06.jpg')
		+slidImg('img/@2x/2017-02-13-news-07.jpg', 'img/@2x/2017-02-13-news-xl-07.jpg')
		+slidImg('img/@2x/2017-02-13-news-08.jpg', 'img/@2x/2017-02-13-news-xl-08.jpg')
		+slidImg('img/@2x/2017-02-13-news-09.jpg', 'img/@2x/2017-02-13-news-xl-09.jpg')
		
	.container
		.strong Технические характеристики:
		.table-wrap
			table
				tr( style="background-color:#16437c; color: #fff").text-center
					th Технические характеристики: <br> Параметр
					th Цилиндрический турборедуктор TG380Т-1 (аналог Р&nbsp;6000)
					th Цилиндрический турборедуктор 210Т-1 (аналог Р&nbsp;3000)
				mixin tr(t1, t2,t3)
					tr.bg-light
						td= t1
						td( ).text-center= t2
						td( ).text-center= t3
				+tr("Передаточное число (i)",'2,7','2,7')
				+tr("Мощность эл. двигателя, кВт",'6 000','3 000')
				+tr("Межосевое расстояние, мм",'380','210')
				+tr("Частота вращения, об. /мин",'3 000','8 100')
				tr.bg-light
						td Система смазки
						td(colspan="2" ).text-center Циркуляционная
						
				tr.bg-light
						td Тип масла
						td(colspan="2" ).text-center ISO VG 32

				+tr("Подача масла, л/мин",'400','230')
				tr.bg-light
						td Характер нагрузки
						td(colspan="2" ).text-center Тяжёлая, равномерная
				tr.bg-light
						td Режим ввода в эксплуатацию
						td(colspan="2" ).text-center Сразу на номинальную нагрузку
				tr.bg-light
						td Климатическое исполнение и категория размещения
						td(colspan="2" ).text-center У1
						
				tr.bg-light
						td Расчетный срок службы, час., не менее
						td(colspan="2" ).text-center 90 000

				+tr("Масса редуктора, кг",'3 000','1 200') 
	.pb-5
		+bread()
	+footer()